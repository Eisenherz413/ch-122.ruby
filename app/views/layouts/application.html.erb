  </head>
  <header>
    <nav class="navbar navbar-expand-md navbar-light bg-light">
      <div class="container-fluid">
        <div class="navbar-collapse collapse" id="collapseNavbar">
          <% if can? :manage, Category %>
            <ul class="navbar-nav h6">
              <li class="nav-item">
                <%= active_link_to t('global.menu.services'),services_path, class: "nav-link"%>
              </li>
              <li class="nav-item">
                <%= active_link_to t('global.menu.rooms'),root_path, class: "nav-link", active: :exclusive%>
              </li>
              <li class="nav-item">
                <%= active_link_to t('global.menu.categories'), categories_path, class: "nav-link"%>
              </li>
            </ul>
          <% elsif (can? :create, Issue) && user_signed_in? %>
            <ul class="navbar-nav h6">

              <li class="nav-item">
                <%= active_link_to t('global.menu.issues'), issues_path, class: "nav-link"%>
              </li>
            </ul>
          <% end %>
        </div>

        <ul class="navbar-nav ms-auto h6">
          <li class="nav-item">
            <%= active_link_to "EN", url_for(locale: :en), class: "nav-link", active: :exact%>
          </li>
          <li class="nav-item">
            <%= active_link_to 'UK', url_for(locale: :uk), class: "nav-link", active: :exact%>
          </li>
          <% if user_signed_in? %>
            <li class="nav-item">
              <strong><%= current_user.email %></strong>.
              <%#= link_to 'Edit profile', edit_user_registration_path, class: "nav-link" %>
            </li>
            <li class="nav-item">
              <%= link_to t("users.sessions.new.log_out"), destroy_user_session_path, method: :delete, class: "nav-link" %>
            </li>
          <% else %>
            <li class="nav-item">
              <%= link_to t("users.sessions.new.sign_up"), new_user_registration_path, class: "nav-link" %>
            </li>
            <li class="nav-item">
              <%= link_to t("users.sessions.new.log_in"), new_user_session_path, class: "nav-link" %>
            </li>
          <% end %>
        </ul>
      </div>
    </nav>
  </header>
  <body>
<!--<div  style="display: inline-block; float: right; margin-top: 30px; margin-right: 30px; margin-left: 10px;"><%#= active_link_to "EN", url_for(locale: :en), class: 'lang'%> <%#= active_link_to 'UK', url_for(locale: :uk), class: 'lang'%></div>-->
  <% unless flash.empty? %>
    <script type="text/javascript">
        <% flash.each do |f| %>
        <% type = f[0].to_s.gsub('alert', 'error').gsub('notice', 'info') %>
        toastr['<%= type %>']('<%= f[1] %>');
        <% end %>
    </script>
  <% end %>

    <%= yield %>
  </body>
</html>
